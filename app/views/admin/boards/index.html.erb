<div class="container">
  <H6>掲示板一覧</H6>
  <div class="row">
    <div>
      <table>
        <% @boards.each do |board| %>
          <tr>
            <td>
              <%= link_to admin_menber_path(board.menber.id) do %>
                <%= image_tag board.menber.get_profile_image(50,50) %>
              <% end %>
            </td>
            <td><%= link_to board.title, admin_board_path(board.id) %></td>
          </tr>
        <% end %>    
      </table>
    </div>      
  </div>    
</div>